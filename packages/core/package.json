{
  "name": "@ldesign-map/core",
  "version": "3.0.0",
  "description": "Core functionality for ldesign-map renderer",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./es/index.d.ts",
      "import": "./es/index.js",
      "require": "./lib/index.cjs"
    },
    "./animation/*": {
      "types": "./es/animation/*.d.ts",
      "import": "./es/animation/*.js",
      "require": "./lib/animation/*.cjs"
    },
    "./editor/*": {
      "types": "./es/editor/*.d.ts",
      "import": "./es/editor/*.js",
      "require": "./lib/editor/*.cjs"
    },
    "./plugin/*": {
      "types": "./es/plugin/*.d.ts",
      "import": "./es/plugin/*.js",
      "require": "./lib/plugin/*.cjs"
    },
    "./renderer/*": {
      "types": "./es/renderer/*.d.ts",
      "import": "./es/renderer/*.js",
      "require": "./lib/renderer/*.cjs"
    },
    "./resource/*": {
      "types": "./es/resource/*.d.ts",
      "import": "./es/resource/*.js",
      "require": "./lib/resource/*.cjs"
    },
    "./spatial/*": {
      "types": "./es/spatial/*.d.ts",
      "import": "./es/spatial/*.js",
      "require": "./lib/spatial/*.cjs"
    },
    "./types/*": {
      "types": "./es/types/*.d.ts",
      "import": "./es/types/*.js",
      "require": "./lib/types/*.cjs"
    },
    "./workers/*": {
      "types": "./es/workers/*.d.ts",
      "import": "./es/workers/*.js",
      "require": "./lib/workers/*.cjs"
    }
  },
  "main": "./lib/index.cjs",
  "module": "./es/index.js",
  "types": "./es/index.d.ts",
  "unpkg": "./dist/index.min.js",
  "jsdelivr": "./dist/index.min.js",
  "files": [
    "README.md",
    "LICENSE",
    "package.json",
    "es",
    "lib",
    "dist"
  ],
  "scripts": {
    "dev": "pnpm --filter @ldesign/builder build && pnpm exec ldesign-builder build --watch",
    "build": "rollup -c",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "clean": "rimraf dist es lib"
  },
  "peerDependencies": {
    "@deck.gl/core": "^9.0.0",
    "@deck.gl/layers": "^9.0.0",
    "@deck.gl/geo-layers": "^9.0.0",
    "@deck.gl/aggregation-layers": "^9.0.0"
  },
  "devDependencies": {
    "@deck.gl/core": "^9.0.0",
    "@deck.gl/layers": "^9.0.0",
    "@deck.gl/geo-layers": "^9.0.0",
    "@deck.gl/aggregation-layers": "^9.0.0",
    "@rollup/plugin-commonjs": "^25.0.0",
    "@rollup/plugin-node-resolve": "^15.2.0",
    "@rollup/plugin-typescript": "^11.1.0",
    "@types/node": "^20.0.0",
    "rimraf": "^5.0.0",
    "rollup": "^4.0.0",
    "tslib": "^2.6.0",
    "typescript": "^5.0.0",
    "vitest": "^1.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}